			<param name="itemTemplates" type="list" >
				<item type="class" key="login" datavar="" class="CTemplateData" source="file" sourceEncoding="windows-1251"     targetEncoding="windows-1251" filename="cms/settings/cms_authorize_files/login.htm"/>
